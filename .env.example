# Optional: set NETWORK to use *_RPC_URL defaults.
# If RPC_URL is set, it wins.
# For multi-network ingest: "sepolia,mainnet"
NETWORK=sepolia,mainnet
RPC_URL=

# Required (optional for NETWORK=sepolia/mainnet)
# Sepolia: 0x92C920834Ec8941d2C77D188936E1f7A6f49c127
# Mainnet: 0xD82385dADa1ae3E969447f20A3164F6213100e75
FHEVM_EXECUTOR_ADDRESS=

# Optional (auto-detected if empty; defaults per NETWORK when available)
# Sepolia: 11155111
# Mainnet: 1
CHAIN_ID=
SEPOLIA_ETH_RPC_URL=
MAINNET_ETH_RPC_URL=
ANVIL_RPC_URL=
START_BLOCK=
END_BLOCK=
CONFIRMATIONS=0
BATCH_SIZE=1000
CATCHUP_MAX_BLOCKS=256
DB_PATH=data/fhevm_stats.sqlite
MODE=both
POLL_INTERVAL_MS=10000

# API server
HTTP_PORT=4310
HTTP_HOST=127.0.0.1
CORS_ALLOW_ORIGINS=
EXPOSE_DB_STATS=0

# API security hardening (optional)
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=240
RATE_LIMIT_DISABLED=0
MAX_PAGE_LIMIT=100
MAX_PAGE_OFFSET=10000
MAX_BLOCK_SPAN=200000
MAX_WINDOW_LOOKBACK=2000
MAX_TOP_LIMIT=100
MAX_EXAMPLE_LIMIT=100
MAX_HORIZON_SIZE=10000
MAX_BUCKET_SIZE=100000
MAX_EVENT_NAME_LENGTH=128


# Smoke test (optional)
SMOKE_START_BLOCK=
SMOKE_END_BLOCK=

# Backfill types (optional)
BACKFILL_LIMIT=

# Rollup (optional)
ROLLUP_START_BLOCK=
ROLLUP_END_BLOCK=
ROLLUP_BLOCK_BATCH=
ROLLUP_BUCKET_SECONDS=1800
